    <validationRules>
        <fullName>Available_Included_in_Recommended</fullName>
        <active>true</active>
        <description>*Recommended for* should contain only elements selected in *Available for* meaning that if an choice is not selected in *Available for*, it shouldn&apos;t be selected for *Recommended for*.</description>
        <errorConditionFormula>(INCLUDES( Recommended_for__c , &quot;Pre-Licensing&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Pre-Licensing&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Provisional License Renewal&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Provisional License Renewal&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Renewal&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Renewal&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Monitoring&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Monitoring&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Progress&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Progress&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Enhanced Monitoring&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Enhanced Monitoring&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Follow-up&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Follow-up&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Change of Location&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Change of Location&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Change of Space&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Change of Space&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;License Upgrade&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;License Upgrade&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Capacity Increase&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Capacity Increase&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Technical Assistance&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Technical Assistance&quot;))) ||
(INCLUDES( Recommended_for__c , &quot;Investigation&quot;) &amp;&amp; NOT( INCLUDES( Available_for__c , &quot;Investigation&quot;)))</errorConditionFormula>
        <errorMessage>An item cannot be in Recommended for unless it is selected in Available for as well.</errorMessage>
    </validationRules>
