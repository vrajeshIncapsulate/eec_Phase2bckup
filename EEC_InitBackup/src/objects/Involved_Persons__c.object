    <validationRules>
        <fullName>Creation_Updates_on_Closed_Case</fullName>
        <active>true</active>
        <description>Block creation and updates on Closed case</description>
        <errorConditionFormula>ISPICKVAL(Involved_In__r.Status,&apos;Closed&apos;) &amp;&amp; !ISBLANK($UserRole.Name) &amp;&amp; !CONTAINS($UserRole.Name,&apos;Director&apos;) &amp;&amp; !CONTAINS($UserRole.Name,&apos;Supervisor&apos;)</errorConditionFormula>
        <errorMessage>Involved Persons can&apos;t be created or updated on &apos;Closed&apos; Cases.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_role_requires_role</fullName>
        <active>true</active>
        <description>If &quot;Other&quot; is chosen in the list of roles, then detail of the role must be entered</description>
        <errorConditionFormula>AND(ISPICKVAL(Role__c, &quot;Other&quot;), OR( Other_Role__c =&quot;&quot;, ISNULL( Other_Role__c), ISBLANK(Other_Role__c)))</errorConditionFormula>
        <errorDisplayField>Other_Role__c</errorDisplayField>
        <errorMessage>Please fill this field as you chose &quot;Other&quot; in the roles.</errorMessage>
    </validationRules>
