        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
var url = parent.location.href;

var visitObj = new sforce.SObject(&quot;Inspection__c&quot;);
visitObj.Id = &apos;{!Inspection__c.Id}&apos;;

var isMasterVisit = {!IF(Inspection__c.Mater_Visit_for_Res_Cluster_of_Visits__c != null, false, true)};

if(isMasterVisit) {
  if(&apos;{!Inspection__c.Status__c}&apos; == &apos;Visit Completed&apos; || &apos;{!Inspection__c.Status__c}&apos; == &apos;Report to be sent to Provider&apos;){ 
     if(&apos;{!JSENCODE(Inspection__c.Cover_Letter_Narrative__c)}&apos; == null || &apos;{!JSENCODE(Inspection__c.Cover_Letter_Narrative__c)}&apos; == &apos;&apos;){
       var provider = sforce.connection.query(&quot;select Id, Name, RecordTypeId,RecordType.DeveloperName,RecordType.Name, Provider_City__r.Name,ShippingStreet,ShippingCity,ShippingState,ShippingPostalCode,Provider_Address_1__c,State__c,Provider_Zip_Code__c,OwnerId, Owner.FirstName, Owner.LastName, Owner.Phone, Umbrella_Members__c, Umbrella_Members__r.Name, Umbrella_Members__r.ShippingStreet,Umbrella_Members__r.ShippingState,Umbrella_Members__r.ShippingPostalCode,Umbrella_Members__r.ShippingCity  from Account where Id = &apos;{!Inspection__c.ProviderId__c}&apos;&quot;); 
       var rtypeName = provider.records.RecordType.DeveloperName; 
       var rtypeLabel = provider.records.RecordType.Name; 
       var director, mainContact = null;
       if (provider.records.Umbrella_Members__c) {
         director = sforce.connection.query(&quot;SELECT Salutation, FirstName, LastName FROM Contact WHERE AccountID = \&apos;&quot;+provider.records.Umbrella_Members__c+&quot;\&apos; AND Role__c = \&apos;Director\&apos; LIMIT 1&quot;);
         mainContact = sforce.connection.query(&quot;SELECT Salutation, FirstName, LastName FROM Contact WHERE AccountID = \&apos;&quot;+provider.records.Umbrella_Members__c+&quot;\&apos; AND Role__c = \&apos;Main Contact / Licensee\&apos; LIMIT 1&quot;);   
       }
       var vList = sforce.connection.query(&quot;select Id, Number_of_non_compliant_items__c from Inspection__c where Id = &apos;{!Inspection__c.Id}&apos; and Number_of_non_compliant_items__c &gt; 0 order by createddate desc&quot;); 
       var numOfChildVisits = sforce.connection.query(&quot;Select Count() From Inspection__c Where Mater_Visit_for_Res_Cluster_of_Visits__c = \&apos;&quot; + visitObj.Id + &quot;\&apos;&quot;);
       var assignedTo;
      
       if(&apos;{!Inspection__c.Inspector_del__c}&apos; != null){
         assignedTo = sforce.connection.query(&quot;select FirstName, LastName, Title from User where Id = &apos;{!Inspection__c.Assigned_To_Id__c}&apos;&quot;);
       }
       
       var directorFullName, mainContactFullName
       // Setup director and main contact name variables.
       if (director != null &amp;&amp; director.size &gt; 0) {
         directorFullName = director.Salutation + &apos; &apos; + director.FirstName + &apos; &apos; + director.LastName;
       }
       
       if (mainContact != null &amp;&amp; mainContact.size &gt; 0) {
         mainContactFullName = mainContact.Salutation + &apos; &apos; + mainContact.FirstName + &apos; &apos; + mainContact.LastName;
       }
       
       var cln = &apos;&apos;;
       
       if (director != null &amp;&amp; director.size &gt; 0 &amp;&amp; rtypeName != &apos;FCC&apos; &amp;&amp; numOfChildVisits.size &gt; 0 ) {
         if (typeof directorFullName != &quot;undefined&quot; &amp;&amp; directorFullName != null) {
           cln += directorFullName + &apos;&lt;br/&gt;&apos;;
         } else {
           cln += provider.records.Umbrella_Members__r.Name != null ? provider.records.Umbrella_Members__r.Name + &apos;&lt;br/&gt;&apos; : &apos;&apos;; 
         }
       }
       
       if (mainContact != null &amp;&amp; mainContact.size &gt; 0 &amp;&amp; rtypeName != &apos;FCC&apos; &amp;&amp; numOfChildVisits.size == 0) {
         if (typeof mainContactFullName != &quot;undefined&quot; &amp;&amp; mainContactFullName != null) {
           cln += mainContactFullName + &apos;&lt;br/&gt;&apos;;
         } else {
           cln += provider.records.Name != null ? provider.records.Name + &apos;&lt;br/&gt;&apos; : &apos;&apos;;
         }
       }
      
       if (rtypeName == &apos;FCC&apos;) {
         cln += provider.records.Name + &apos;&lt;br/&gt;&apos;;
       }
    
       if (numOfChildVisits.size == 0) {
         cln = cln + (provider.records.ShippingStreet != null ? provider.records.ShippingStreet : provider.records.Provider_Address_1__c) + &apos;&lt;br/&gt;&apos;;
         cln = cln + (
           provider.records.ShippingStreet != null 
             ? (provider.records.ShippingCity + &apos;, &apos; + provider.records.ShippingState + &apos;, &apos; + provider.records.ShippingPostalCode) 
             : (provider.records.Provider_City__r.Name + &apos;, &apos; + provider.records.State__c + &apos;, &apos; + provider.records.Provider_Zip_Code__c)
         ) + &apos;&lt;br/&gt;&lt;br/&gt;&apos;;   
       } else {
         cln = cln + (provider.records.Umbrella_Members__c != null &amp;&amp; provider.records.Umbrella_Members__r.ShippingStreet != null ? provider.records.Umbrella_Members__r.ShippingStreet : provider.records.Provider_Address_1__c) + &apos;&lt;br/&gt;&apos;;
         cln = cln + (
           provider.records.Umbrella_Members__c != null &amp;&amp; provider.records.Umbrella_Members__r.ShippingStreet != null 
             ? (provider.records.Umbrella_Members__r.ShippingCity + &apos;, &apos; + provider.records.Umbrella_Members__r.ShippingState + &apos;, &apos; + provider.records.Umbrella_Members__r.ShippingPostalCode) 
             : (provider.records.Provider_City__r.Name + &apos;, &apos; + provider.records.State__c + &apos;, &apos; + provider.records.Provider_Zip_Code__c)
         ) + &apos;&lt;br/&gt;&lt;br/&gt;&apos;;
       }
       
       cln += &apos;&lt;div&gt;Re: Visit conducted &apos;;
      
       var dateStr = &apos;{!Inspection__c.Date_of_Visit__c}&apos;;
       if (typeof dateStr != &apos;undefined&apos; &amp;&amp; dateStr != &apos;&apos;) {
         cln += &apos;on&amp;nbsp;{!Inspection__c.Date_of_Visit__c}&apos;;  
       }
      
       if (rtypeName != &apos;FCC&apos;) {
         cln += &apos; for &apos; + provider.records.Name + &apos;&lt;/div&gt;&lt;br/&gt;&apos;;   
       } else {
         cln += &apos;&lt;/div&gt;&lt;br/&gt;&apos;
       }
    
       if(rtypeName == &apos;FCC&apos;){
         cln += &apos;Dear &apos;+provider.records.Name + &apos;,&lt;br/&gt;&lt;br/&gt;&apos;; 
       } else {
         if (numOfChildVisits.size &gt; 0) {
           cln += (typeof directorFullName != &apos;undefined&apos; &amp;&amp; directorFullName != &apos;&apos; ? &apos;Dear &apos; + directorFullName : &apos;Dear Provider&apos;) + &apos;,&lt;br/&gt;&lt;br/&gt;&apos;;
         } else {
           cln += (typeof mainContactFullName != &apos;undefined&apos; &amp;&amp; mainContactFullName != &apos;&apos; ? &apos;Dear &apos; + mainContactFullName : &apos;Dear Provider&apos;) + &apos;,&lt;br/&gt;&lt;br/&gt;&apos;;
         }
       }
      
       if(rtypeName == &apos;FCC&apos;){
           cln = cln + &apos;Attached please find the results from my recent {!Inspection__c.Type_of_Visit__c} visit to your &apos;+rtypeLabel+&apos; program.&lt;br/&gt;&lt;br/&gt;&apos;;
       } else {
          cln = cln + &apos;Attached please find the report resulting from my recent {!Inspection__c.Type_of_Visit__c} visit to your &apos;+rtypeLabel+&apos; program.&lt;br/&gt;&lt;br/&gt;&apos;;
       }
 
       if(vList != null &amp;&amp; vList.size&gt;0){
          cln = cln +&apos;As you will note, there are non-compliances which must be corrected. Therefore please read carefully the attached report and submit required corrective action by &apos;+ &apos;{!Inspection__c.Corrective_Action_Deadline__c}&apos;+&apos;.&lt;br/&gt;&apos;;
          if(&apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Pre-Licensing&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Provisional License Renewal&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Renewal&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Change of Location&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;License Upgrade&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Capacity Increase&apos;){
             cln = cln + &apos;Following receipt of an acceptable corrective action plan and assuming all other required documentation has been received, your new license will be issued.&lt;br/&gt;&lt;br/&gt;&apos;;
          }
       } else {
         cln = cln + &apos;There are no non-compliances which need to be corrected.&apos;;
         if(&apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Pre-Licensing&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Provisional License Renewal&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Renewal&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Change of Location&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;License Upgrade&apos; || &apos;{!Inspection__c.Type_of_Visit__c}&apos; == &apos;Capacity Increase&apos;){
           cln = cln + &apos;Therefore assuming all required documentation has been received, your new license will be issued and sent via U.S Mail.&apos;;
         }
         cln = cln + &apos; Please keep this report for your records.&lt;br/&gt;&lt;br/&gt;&apos;;
       }
 
       cln = cln + &apos;If you have any questions regarding this report, please feel free to contact me at the address: {!Inspection__c.Assigned_To_Email__c} or by phone: {!Inspection__c.Assigned_To_Phone__c}.&lt;br/&gt;&lt;br/&gt;Respectfully,&lt;br/&gt;&apos;;
       if(assignedTo != null){
         cln = cln + assignedTo.records.FirstName+&apos; &apos;+assignedTo.records.LastName+&apos;&lt;br/&gt;&apos;;
         cln = cln + (assignedTo.records.Title != null?assignedTo.records.Title:&apos;&apos;)+&apos;&lt;br/&gt;&apos;;
       }
       visitObj.Cover_Letter_Narrative__c = cln;
       var result = sforce.connection.update([visitObj]);
       window.location.reload();
     } else {
       alert(&apos;Cover Letter Narrative is not empty. If you want to use this functionality, empty the field prior clicking on this button&apos;);
     }
  }else {
    alert(&apos;This functionality is only available if the visit is in status &quot;Visit Completed&quot; or &quot;Report to be sent to Provider&quot;&apos;);
  }
} else {
  alert(&apos;This visit has a master, please use the master to set the defaults&apos;);
}</url>
