        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)} 
var url = parent.location.href; 

var iItems = sforce.connection.query(&quot;select Id,RecordTypeId,Associated_regulation__c,Observations__c, Standard_Inspection_Item__c,Additional_Inspection_Item_Name__c, Technical_Assistance__c, Inspection_No__c,Inspection_No__r.Date_of_Visit__c,Discussed__c,Correction_Plan__c,Date_Corrective_Actions_Received__c from Inspection_Item__c where Inspection_No__c = &apos;{!Inspection__c.Id}&apos; AND Inspection_Result__c = &apos;Non-Compliant&apos;&quot;); 

iiList = iItems.getArray(&quot;records&quot;); 

if(iiList.length &gt; 0){ 

var lvRType = sforce.connection.query(&quot;select Id from recordType where DeveloperName = &apos;Licensing_Visits&apos; and sobjectType = &apos;Inspection__c&apos;&quot;); 

lvrList = lvRType.getArray(&quot;records&quot;); 
var insp = new sforce.SObject(&quot;Inspection__c&quot;); 

insp.Status__c = &apos;To be Scheduled&apos;; 
insp.Provider_No__c = &apos;{!Inspection__c.ProviderId__c}&apos;; 
insp.Investigation__c = &apos;{!Inspection__c.InvestigationId__c}&apos;; 
insp.Original_Visit__c = &apos;{!Inspection__c.Id}&apos;; 
insp.Type_of_Visit__c = &apos;Follow-Up&apos;; 
insp.RecordTypeId = &apos;{!Inspection__c.RecordTypeId}&apos;; 

if(&apos;{!Inspection__c.Assigned_To_Id__c}&apos; != &apos;&apos;){ 
insp.Inspector_del__c = &apos;{!Inspection__c.Assigned_To_Id__c}&apos;; 
} 
else{ 
insp.Inspector_del__c = &apos;{!Inspection__c.Provider_Owner__c}&apos;; 
} 


if(&apos;{!Inspection__c.Date_of_Visit__c}&apos; != null &amp;&amp; &apos;{!Inspection__c.Date_of_Visit__c}&apos; != &apos;&apos;){ 

var provider = sforce.connection.query(&quot;select Id, RecordTypeId,RecordType.Name from Account where Id = &apos;{!Inspection__c.Provider_NoId__c}&apos;&quot;); 
var rtypeName = provider.records.RecordType.Name; 
var leadConf = sforce.connection.query(&quot;select Id,name, Follow_up_Visit_Estimated_Deadline__c from LEAD_Configuration__c where name = \&apos;&quot;+rtypeName+&quot;\&apos;&quot;); 

if(leadConf != null &amp;&amp;leadConf.records != null){ 
var dt = new Date(&apos;{!Inspection__c.Date_of_Visit__c}&apos;); 
dt.setDate(dt.getDate()+parseInt(leadConf.records.Follow_up_Visit_Estimated_Deadline__c)); 
var date1 =((dt.getMonth()+1)+&quot;/&quot;+dt.getDate()+&quot;/&quot;+dt.getFullYear()); 
insp.Estimated_Deadline__c = new Date(date1); 
} 

} 
var result=sforce.connection.create([insp]); 

if (result[0].getBoolean(&quot;success&quot;) == false ) 
{ 

alert(&quot;An error occurred creating a Follow up Visit.&quot;+result[0].get(&quot;errors&quot;)); 
} 
else{ 

iiList = iItems.getArray(&quot;records&quot;); 
var insertII = []; 

for (var i=0; i&lt; iiList.length; i++) { 
var tiItem = iiList[i]; 
var iItem = new sforce.SObject(&quot;Inspection_Item__c&quot;); 
iItem.RecordTypeId = tiItem.RecordTypeId; 
iItem.Associated_regulation__c = tiItem.Associated_regulation__c; 
iItem.Standard_Inspection_Item__c = tiItem.Standard_Inspection_Item__c; 
iItem.Additional_Inspection_Item_Name__c=tiItem.Additional_Inspection_Item_Name__c; 
iItem.Observations__c = &quot;Observations from original visit on &quot;+tiItem.Inspection_No__r.Date_of_Visit__c; 
if(tiItem.Observations__c!=&apos;&apos;){ 
iItem.Observations__c = iItem.Observations__c +&quot;: &quot;+tiItem.Observations__c;
} 
if(tiItem.Correction_Plan__c!=&apos;&apos;){ 
iItem.Observations__c = iItem.Observations__c + &quot;\n&quot; +&quot;Corrective Plan received on &quot;+tiItem.Date_Corrective_Actions_Received__c+&quot;: &quot;+tiItem.Correction_Plan__c; 
} 
iItem.Technical_Assistance__c = tiItem.Technical_Assistance__c; 
iItem.Inspection_No__c = result[0].id; 
iItem.Discussed__c = tiItem.Discussed__c; 


insertII.push(iItem); 
} 

var result1=sforce.connection.create(insertII); 
if (result1[0].getBoolean(&quot;success&quot;) == false ) 
{ 
alert(&quot;Testhere&quot;); 
alert(&quot;An error occurred creating a Follow up Visit1.&quot;+result1[0].get(&quot;errors&quot;)); 
}else{ 
window.location = &apos;/&apos;+result[0].id; 
} 
} 


} 
else{ 
alert(&quot;Cannot create a Follow-up as there are no &apos;Non-Complaint&apos; items for this Visit&quot;); 
}</url>
