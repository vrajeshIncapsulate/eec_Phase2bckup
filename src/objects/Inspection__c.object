    <fields>
        <fullName>Count_Visit_For_Visit_Creation_Batch__c</fullName>
        <description>If checked, it means that this visit counts as a visit that can be taken into account during the automated visit creation batch, which would mean that a new visit would not be created by the system</description>
        <externalId>false</externalId>
        <formula>(ISBLANK( Date_of_Visit__c ) &amp;&amp;

CONTAINS(RecordType.Name, &quot;Licensing&quot;) &amp;&amp;

(ISPICKVAL(Type_of_Visit__c , &quot;Monitoring&quot;) || ISPICKVAL(Type_of_Visit__c , &quot;Enhanced Monitoring&quot;) || ISPICKVAL(Type_of_Visit__c , &quot;Provisional License Renewal&quot;) || ISPICKVAL(Type_of_Visit__c , &quot;Progress&quot;) )  &amp;&amp;

NOT(ISPICKVAL(Status__c , &quot;Cancelled&quot;)))

||

(DATEVALUE(CreatedDate) &gt;= TODAY() - 5 &amp;&amp;  Visit_Created_By_Batch__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Count Visit For Visit Creation Batch</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
