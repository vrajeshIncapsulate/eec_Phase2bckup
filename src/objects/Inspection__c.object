if(visitReasons.indexOf(&apos;{!Inspection__c.Type_of_Visit__c}&apos;) &gt; -1 &amp;&amp; (&apos;{!Inspection__c.Visit_Tool_Used__c}&apos; == null || &apos;{!Inspection__c.Visit_Tool_Used__c}&apos; == &apos;&apos;)){ 
