        <protected>false</protected>
        <url>try { 
// If the current visit tool is null. 
if({!Inspection__c.Offline_Lock__c}){ 
throw &quot;This visit is being used in the offline visit app and is locked. Go to the offline visit app and upload the visit before selecting this action.&quot;; 
} 
if (&apos;{!Inspection__c.Visit_Tool_Used__c}&apos; == &apos;&apos;) { 
throw &quot;A visit tool must be selected before you can switch to another tool.&quot;; 
} 
if(&apos;{!Inspection__c.Abandon_Tool_Reason__c}&apos; != &apos;&apos;){ 
throw &quot;Visit tool switch is not available if a visit tool has been previously abandoned&quot;; 
} 
var visitStatus = &apos;{!Inspection__c.Status__c}&apos;; 
visitStatus = visitStatus.toLowerCase(); 
console.log(&apos;Visit Status: &apos; + visitStatus); 
// If the visit has been scheduled or completed. 
if (visitStatus == &apos;to be scheduled&apos; || visitStatus == &apos;scheduled&apos; || visitStatus == &apos;visit completed&apos;) { 

// If the maximum number of switches has been hit. 
var userroleName = &apos;{!$UserRole.Name}&apos;; 
userroleName = userroleName.toLowerCase(); 
if (&apos;{!Inspection__c.Switch_Tool_Justification_1__c}&apos; != &apos;&apos; &amp;&amp; &apos;{!Inspection__c.Switch_Tool_Justification_2__c}&apos; != &apos;&apos; &amp;&amp; userroleName.indexOf(&apos;director&apos;) &lt; 0 &amp;&amp; userroleName.indexOf(&apos;supervisor&apos;) &lt; 0){ 
throw &quot;Tool has already been switched twice and cannot be switched again. Contact your supervisor if you need to switch the tool again.&quot;; 
} 

// Success flow. =&gt; Go to page with Visit ID and flag to inform of display type. 
window.location.href = &apos;/apex/EEC_VisitToolSwitchSelection?id={!Inspection__c.Id }&amp;mode=1&apos;; 
} else { 
throw &quot;Visit has already been conducted. Tool used cannot be switched.&quot;; 
} 
} catch(e) { 
// Alert the custom error message. 
alert(e); 
}</url>
