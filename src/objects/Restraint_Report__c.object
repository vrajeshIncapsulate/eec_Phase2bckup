    <validationRules>
        <fullName>Report_to_match_Provider_RestraintMethod</fullName>
        <active>true</active>
        <errorConditionFormula>IF( AND( RecordType.Name = &apos;DYS Restraint&apos; , TEXT(Provider__r.Restraint_Method__c) = &apos;DYS&apos; ) , False , 
  IF( AND( RecordType.Name = &apos;Varied Restraint&apos; , TEXT(Provider__r.Restraint_Method__c) = &apos;Varied&apos;), False,
  IF( AND( RecordType.Name = &apos;Other Restraint&apos; , TEXT(Provider__r.Restraint_Method__c) &lt;&gt; &apos;DYS&apos;,TEXT(Provider__r.Restraint_Method__c) &lt;&gt; &apos;Varied&apos;), False, True)))</errorConditionFormula>
        <errorMessage>The restraint report doesn&apos;t match the restraint method on the provider.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Year_is_correct</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(AND(ISNUMBER( Calendar_Year__c ), VALUE(Calendar_Year__c) &gt; 1900, VALUE(Calendar_Year__c) &lt;= 2100))</errorConditionFormula>
        <errorDisplayField>Calendar_Year__c</errorDisplayField>
