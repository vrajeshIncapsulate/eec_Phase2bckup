    <validationRules>
        <fullName>Cor_Plan_Status_is_NULL_if_Result_is_NC</fullName>
        <active>true</active>
        <description>The Correction Plan Status must be = &quot;--None--&quot; if the Visit Item &quot;Result&quot; is = &quot;Non-Compliant&quot;</description>
        <errorConditionFormula>NOT(ISBLANK(TEXT(Correction_Plan_Status__c)))
&amp;&amp;
NOT(ISPICKVAL(Inspection_Result__c, &quot;Non-Compliant&quot;))</errorConditionFormula>
        <errorDisplayField>Correction_Plan_Status__c</errorDisplayField>
        <errorMessage>This status is only used for Non-Compliant Visit Items</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Correction_Plan_NULL_Date_Impl_NOT_Null</fullName>
        <active>true</active>
        <description>If Correction Plan is not empty then Date Implemented should not be empty</description>
        <errorConditionFormula>NOT( ISBLANK( Correction_Plan__c ) )  &amp;&amp;  ISBLANK( Date_Implemented__c ) &amp;&amp; ISBLANK( Inspection_No__r.Legacy_FCC_LM_Mobile_Visit__c )</errorConditionFormula>
        <errorDisplayField>Date_Implemented__c</errorDisplayField>
        <errorMessage>If Correction Plan is provided then Date Implemented is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Impl_NULL_Cor_Plan_NOT_NULL</fullName>
        <active>true</active>
        <description>If  Date Implemented is not empty then  Correction Planshould not be empty</description>
        <errorConditionFormula>NOT( ISBLANK( Date_Implemented__c ) )  &amp;&amp;  ISBLANK( Correction_Plan__c )</errorConditionFormula>
        <errorDisplayField>Correction_Plan__c</errorDisplayField>
        <errorMessage>If Date Implemented is provided then Correction Plan is mandatory</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Cor_Plan_or_Sub_NULL_EEC_fdbk_Null</fullName>
        <active>true</active>
        <description>&quot;Corrective Action EEC Feedback&quot; should stay empty if: Correction Plan is NULL or Corrective Actions Submitter is NULL</description>
        <errorConditionFormula>NOT(ISBLANK(Corrective_Action_EEC_Feedback__c)) &amp;&amp; (ISBLANK(Corrective_Actions_Submitter__c))</errorConditionFormula>
        <errorDisplayField>Corrective_Action_EEC_Feedback__c</errorDisplayField>
        <errorMessage>If Correction Plan has not been submitted and entered in LEAD, Corrective Action Feedback can not be filled. Enter first a correction plan and then add feedback.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Result_for_Non_Assessed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Non_Assessed_Item__c, !ISPICKVAL(Inspection_Result__c,&apos;&apos;))</errorConditionFormula>
        <errorDisplayField>Inspection_Result__c</errorDisplayField>
        <errorMessage>Non-Assessed visit items should not be assessed and their assessment result should remain empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_updates_allowed_on_legacy_visit_data</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Inspection_No__r.RecordType.DeveloperName = &apos;Legacy_System_Import_Visit_Non_Compliance&apos;
,  OR(ISCHANGED( Category_additional__c ) 
,  ISCHANGED( Key_Indicator__c ) 
,  ISCHANGED( Description_of_additional_inspection__c ) 
,  ISCHANGED( Assessed_Date__c ) 
,  ISCHANGED( Inspection_Result__c )
,  ISCHANGED( Observations__c )
,  ISCHANGED( Technical_Assistance__c )
,  ISCHANGED( Correction_Plan__c )
,  ISCHANGED( Correction_Plan_Status__c )
,  ISCHANGED( Corrective_Actions_Submitter__c )
,  ISCHANGED( Corrective_Action_EEC_Feedback__c )
,  ISCHANGED( Date_Implemented__c )
,  ISCHANGED( Date_Corrective_Actions_Received__c )
,  ISCHANGED( Date_Correction_Plan_Accepted__c )
,  ISCHANGED( Last_Corrective_Action_EEC_Feedback__c )
,  ISCHANGED( Visit_Mode_Ranking__c )
, ISCHANGED( Observations__c ))
)</errorConditionFormula>
        <errorMessage>No updates allowed on legacy data.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Regulation_match_provider_type</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.Name = &quot;Non Standard Visit Item&quot;, Associated_regulation__c != null,NOT(OR(
(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Adoption Agency&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Adoption Agency&apos;), (INCLUDES(Associated_regulation__r.Applicable_To__c, &apos;Family Child Care&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Family Child Care&apos;), (INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Foster Care Agency&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Foster Care Agency&apos;), (INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Group Care&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Group Care&apos;),(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Large Group&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Large Group&apos;),(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Large and Small Group Umbrella&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Large and Small Group Umbrella&apos;),(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;R&amp;P Umbrella&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;R&amp;P Umbrella&apos;),(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Small Group&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Small Group&apos;),
(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Temporary Shelter&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Temporary Shelter&apos;),
(INCLUDES(Associated_regulation__r.Applicable_To__c,&apos;Unlicensed Provider&apos;) &amp;&amp; Inspection_No__r.Provider_No__r.RecordType.Name = &apos;Unlicensed Provider&apos;))
))</errorConditionFormula>
        <errorDisplayField>Associated_regulation__c</errorDisplayField>
        <errorMessage>Regulation associated with this item does not  match the provider type. Please select another regulation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Visit_Items_NC_and_Observation_NULL</fullName>
        <active>true</active>
        <description>If the user tries to save a visit item that is (&apos;Non-compliant&apos;, &apos;Not Applicable&apos; or &apos;Not Assessed&apos;). and &quot;Observations&quot; empty it will show an error message</description>
        <errorConditionFormula>AND(OR(ISBLANK( Observations__c ),ISNULL( Observations__c )), 
OR(ISPICKVAL( Inspection_Result__c , &quot;Non-Compliant&quot;),ISPICKVAL( Inspection_Result__c , &quot;Not Assessed&quot;),ISPICKVAL( Inspection_Result__c , &quot;Not Applicable&quot;)) )</errorConditionFormula>
        <errorDisplayField>Observations__c</errorDisplayField>
        <errorMessage>Observations should be provided for items marked as &apos;Non-compliant&apos;, &apos;Not Applicable&apos; or &apos;Not Assessed&apos;.</errorMessage>
    </validationRules>
</CustomObject>
