            <div id="incident" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div id="no-more-tables" class="table-group-one">
                  <table class="table table-hover">
                      <thead>
                          <tr>
                            <th>Number</th>
                            <th>Summary</th>
                            <th>Event Date</th>  
                            <!--<th>Status</th>-->
                            <th>Submitted Date</th> 
                            <th>Status</th>  
                            <!--<th>Date Closed</th>-->  
                            <th></th>    
                          </tr>
                        </thead>  
                          <apex:repeat value="{! incidentlist }" var="inc" >  
                              <tr>
                                <td data-title="Number">{!inc.CaseNumber}</td>
                                <td data-title="Summary">{!inc.Subject}</td>                                
                                <td data-title="Event Date">
                                <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                    <apex:param value="{!inc.Date_Event_Occured__c}" />
                                </apex:outputText>                               
                                </td>
                                <!--<td data-title="Status">{!inc.Status}</td>-->
                                <td data-title="Date Opened"> 
                                <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                    <apex:param value="{!inc.CreatedDate}" />
                                </apex:outputText>                               
                                </td>
                                <!--<td data-title="Date Closed">
                                <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                    <apex:param value="{!inc.ClosedDate}" />
                                </apex:outputText>                               
                                </td>-->
                                 <td data-title="Status">{!IF((inc.Status != 'Not Submitted' && inc.Status != 'closed'),'Submitted',
                                     IF(inc.status = 'closed','Closed','Not Submitted'))}</td>    
                                                         
                                <td data-title="View" style="text-align:right;">
                                    
                                    <!-- <a href= "/EEC_CasesIncidentsPage?Id={!inc.Id}&mode=edit{!IF(memberId != null ,'&member='+memberId,'')}" target="_top" title="View"><i class="fa fa-search"></i></a>
                                    <a href= "/servlet/servlet.FileDownload?file={!AttachmentId}" target="_top" title="View"><i class="fa fa-search"></i></a>    
                                    -->
                                    <apex:outputText rendered="{!attachmap[inc.Id] == 'None' && inc.Status == 'Not Submitted'}">
                                        <a href= "/EEC_CasesIncidentsPage?Id={!inc.Id}&mode=edit{!IF(memberId != null ,'&member='+memberId,'')}" target="_top" title="View"><i class="fa fa-search"></i></a>
                                    </apex:outputText>
                                    <apex:outputText rendered="{!attachmap[inc.Id] != 'None' && inc.Status == 'Not Submitted'}">
                                        <a href= "/servlet/servlet.FileDownload?file={!attachmap[inc.Id]}" target="_top" title="View"><i class="fa fa-search"></i></a>    
                                    </apex:outputText>
                                    
                                </td> 
                              
                              </tr>
                          </apex:repeat>
                  </table>
                </div>
             </div>
            </div>
           </div>
            
            <div class="panel panel-default {!IF(account.Recordtype.Name != 'Family Child Care', '', 'not-visible')}">
             <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#intervention" href="#intervention" aria-expanded="false" aria-controls="intervention" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Internal Investigation

                </a>
              </h4>
            </div>
            <div id="intervention" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div id="no-more-tables" class="table-group-one">
                  <table class="table table-hover">
                      <thead>
                          <tr>
                            <th>Investigation ID</th>
                            <th>Instructions</th>
                            <th>To be provided before</th>  
                            <th></th>                            
                          </tr>
                      </thead>
                          <apex:repeat value="{! interventionlist }" var="int" >  
                              <tr>
                                <td data-title="ID">{!int.Investigation__r.Name}</td>
                                <td data-title="Instructions">{!int.Portal_User_Instructions__c}</td>
                                <td data-title="To be provided before">
                                <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                    <apex:param value="{!int.To_Be_Provided_By__c}" />
                                </apex:outputText>
                                </td>
                                <td data-title="Upload Internal Investigation" style="text-align:right;"><a href="/EEC_Interventionpage?Id={!int.Id}{!IF(memberId != null,'&member='+memberId,'')}" target="_top" title="Upload"><i class="fa fa-upload"></i></a></td>
                              </tr>
                          </apex:repeat>
                  </table>
                  </div>    
              </div>
             </div>
            </div>
            
           <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#investigation" href="#investigation" aria-expanded="false" aria-controls="investigation" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Investigations

                </a>
              </h4>
            </div>
            <div id="investigation" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                   <div id="no-more-tables" class="table-group-one">
                  <table class="table table-hover">
                      <thead>
                          <tr>
                            <th>Investigation ID</th>
                            <th>Status</th>
                            <th>Closed Date</th>  
                            <th></th>                            
                          </tr>
                      </thead>
                          <apex:repeat value="{! investlist }" var="inv" >  
                              <tr>
                                <td data-title="ID">{!inv.Name}</td>
                                <td data-title="Status">{!inv.Investigation_Status__c}</td>
                                <td data-title="Closed Date">
                                <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                    <apex:param value="{!inv.Closed_Date__c}" />
                                </apex:outputText>
                                </td>
                                <td data-title="" style="text-align:right;"><a href="/EEC_PortalInvestigationPage?Id={!inv.Id}{!IF(memberId != null,'&member='+memberId,'')}" target="_top" title="View"><i class="fa fa-search"></i></a></td>
                              </tr>
                          </apex:repeat>
                  </table>
                  </div>
             </div>
            </div>
           </div> 
            
            <div class="panel panel-default {!IF(account.Recordtype.Name ='Group Care' || account.Recordtype.Name ='Temporary Shelter' && account.Use_Physical_Restraints__c = true, '','not-visible')}">
             <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#report" href="#report" aria-expanded="false" aria-controls="report" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Quarterly Restraint Report

                </a>
              </h4>
            </div>
            <div id="report" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div id="no-more-tables">
                  <table class="table table-hover">
                      <thead>
                          <tr>
                            <th>Transaction ID</th>
                            <th>Report Year</th>
                            <th>Report Quarter</th> 
                            <th>Submitted By</th> 
                            <th>Submission Date</th>
                            <th>Restraint Method</th>
                            <th>Total Restraint</th>
                            <th></th>                            
                          </tr>
                      </thead>
                          <apex:repeat value="{! reportlist }" var="rep" >  
                              <tr>
                                <td data-title="ID">{!rep.Name}</td>
                                <td data-title="Report Year">{!rep.Calendar_Year__c}</td>
                                <td data-title="Report Quarter">{!rep.Quarter__c}</td>
                                <td data-title="Submitted By">{!rep.Contact_Submitted_By__r.name}</td>
                                <td data-title="Date">
                                    <apex:outputText value="{0, date, MMMM d','  yyyy}">
                                    <apex:param value="{!rep.Submitted_Date__c}" />
                                    </apex:outputText>
                                </td>
                                <td data-title="Restraint Method">{!rep.Restraint_Method2__c}</td>
                                <td data-title="Total">{!rep.Grand_Total__c}</td>
                                <td data-title="" style="text-align:right;"><a href="/EEC_RestraintReportDetailPage?Id={!rep.Id}{!IF(memberId != null,'&member='+memberId,'')}" target="_top" title="View"><i class="fa fa-search"></i></a></td>                                
                              </tr>
                          </apex:repeat>
                  </table>
                  </div>
              </div>
             </div>
            </div>  
        </div>
      </apex:pageBlock>
      </apex:outputPanel>
      </div>
      </div>
      </apex:form>
      </div>
      <c:EEC_Site_Footer id="siteFooter" />   
 </body>
</apex:page>