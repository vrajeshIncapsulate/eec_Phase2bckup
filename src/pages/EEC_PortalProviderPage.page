<apex:page showHeader="false" standardStylesheets="false" sidebar="false" controller="EEC_PortalMyProviderPageController">
  <head>
    <title>EEC : Portal Provider Page</title>
    <c:EEC_Header id="pageHeader" />

    <script>
        
    </script>
      <style type="text/css">
        .form-label{
          font-weight: bold;
          font-size:14px;
        }
        thead{
                background-color : #f5f5f5;
        }
          
        .header-button {
          margin-right: 3px; 
        }
        .not-visible{
          display : none;
        }
        .form-group{
            width : 100%;
        }
        .btn-group{
            margin-bottom : 100px;
        }
        .table > thead:first-child > tr:first-child > th{
            border-bottom: 0px;
            border-top: 1px solid #dddddd;
            font-weight: bold;
            font-size: 14px;
        }
        .table {
            border-left : 1px solid #dddddd;
            border-right : 1px solid #dddddd;
            border-bottom : 1px solid #dddddd;
         }
        .licensing-table > thead:first-child > tr:first-child > th{
            width: 135.43px;
         }
        .table-group-one .table > thead:first-child > tr:first-child > th{
            width: 135.43px;
         }
         .table-group-two .table > thead:first-child > tr:first-child > th{
            width: 189.6px;
         }  
        .provider-container{
          margin-bottom : 100px;
          } 
          @media only screen and (max-width: 800px) { 
          .right-group
              {
                padding-right : 3px;
                width:100%;
              }
          }
          
          @media only screen and (max-width: 959px) and (min-width: 0px){
            .navigation-buttons input,a {
                font-size : 12px;
            }
          }
      </style>
  </head>
  <body>
      <c:EEC_Nav_Bar id="navBar" />
      <div class="container">
        <apex:form >
        <div class="provider-container" layout="row" layout-margin="true" layout-padding="true" layout-align="center center" flex="100">
        <div class="provider-information" layout="column" flex="90" flex-sm="90">
        <apex:outputPanel rendered="{!errorMsg != null}">
          <h2>{!errorMsg}</h2>  
        </apex:outputPanel>    
            
        <apex:outputPanel id="userDetail">    
            <br/>
            <h2 class="pull-left col-lg-4">Provider</h2>
            <br/><br/><br/><br/>
            <div class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">    
            <div class="right-group md-button">
                <apex:commandButton action="{!edit}" styleClass="md-button md-raised md-eec header-button" rendered="{!!isEdit && profileName != 'Community Standard User without transactions for EEC'}"  value="{!$Label.site.edit}" id="editButton"/>      
            </div>
            <div class="right-group pull-right md-button {!IF((account.Recordtype.Name = 'Group Care' || account.Recordtype.Name = 'Temporary Shelter') && account.Use_Physical_Restraints__c = true && isEdit == false, '', 'not-visible')}">
                <apex:commandButton html-layout-fill="true" styleClass="md-button md-raised md-eec" value="Submit New Quarterly Restraint Report"  action="{!resReport}"></apex:commandButton>
            </div>
                
            <div class="right-group pull-right md-button {!IF(!isEdit, '' ,'not-visible')}">
                <apex:commandButton html-layout-fill="true" styleClass="md-button md-raised md-eec" value="Submit New Incident / Injury"  action="{!subInc}"></apex:commandButton>
            </div>
            <div class="right-group pull-right md-button {!IF(!isEdit, '' ,'not-visible')}">
                <apex:commandButton action="{!noncomp}" styleClass="md-button md-raised md-eec header-button" rendered="{!!isEdit}"  value="Fill Non-Compliance Corrective Actions" id="noncompButton"/>      
            </div>    
            <div class="right-group pull-right md-button {!IF(!isEdit, '' ,'not-visible')}">    
                <apex:outputpanel styleClass="right-group" rendered="{!IF(account.Recordtype.Name ='Family Child Care',true,false)}">
                  <apex:commandButton action="{!trxIntro}" styleClass="md-button md-raised md-eec header-button" rendered="{!!isEdit && profileName != 'Community Standard User without transactions for EEC'}"  value="New Licensing Transaction" id="licenseButton"/>      
                </apex:outputpanel>
            </div>
            </div>
            <br/>
        <apex:pageBlock id="editUserDetail" rendered="{!isEdit}">
            <apex:pageMessages /> 
            <section class="contact-information">
                        <h4 class="md-title">Provider Information</h4>
                        <md-divider></md-divider><br/>
                        <div class="row">
                        <div class="col-lg-6">
                            <div class="md-subhead" layout="row">
                                <p flex-sm="80" flex="90"><strong>Name: </strong>{!account.name}</p>
                            </div>
                        </div>
                            <div class="col-lg-6">
                                <div class="md-subhead" layout="row">
                                    <p flex-sm="80" flex="90"><strong>Type:</strong> {!account.Recordtype.Name}</p>
                                </div> 
                            </div>    
                        </div>
                    </section>
                    
                    <section class="contact-information">
                        <h4 class="md-title">Contact Information</h4>
                        <md-divider></md-divider><br/>
                        <div class="row">
                        <div class="col-lg-6">
                            <div class="md-subhead" layout="row">
                                <div class="form-group">
                                   <label>Phone : </label>
                                   <apex:inputField styleClass="form-control" value="{!account.Phone}" />
                               </div>
                            </div>
                            <div class="md-subhead" layout="row">    
                               <div class="form-group">
                                   <label>Alternate Phone : </label>
                                   <apex:inputField styleClass="form-control" value="{!account.Alternate_Phone__c}"/>
                               </div>
                            </div>
                        </div>
                            <div class="col-lg-6">
                                <div class="md-subhead" layout="row">
                                     <div class="form-group">
                                      <label>Fax : </label>
                                       <apex:inputField styleClass="form-control" value="{!account.fax}" />
                                    </div> 
                                </div>   
                                <div class="md-subhead" layout="row">    
                                    <div class="form-group">
                                      <label>Email : </label>
                                       <apex:inputField styleClass="form-control" value="{!account.Provider_Email__c}" />
                                    </div>
                                </div> 
                            </div>    
                        </div>
                    </section>
                    <section class="contact-information">
                        <h4 class="md-title">Main Address Information</h4>
                        <md-divider></md-divider><br/>
                        <div class="row">
                        <div class="col-lg-6">
                            <div class="md-subhead" layout="row">
                                <div class="form-group">
                                   <label>Street Address 1 :&nbsp;</label>
                                   <apex:outputField styleClass="form-control" value="{!account.Provider_Address_1__c}" label="Contact Name"/>
                               </div>
                            </div>
                            <div class="md-subhead" layout="row">    
                               <div class="form-group">
                                   <label>Street Address 2 :&nbsp;</label>
                                   <apex:outputField styleClass="form-control" value="{!account.Provider_Address_2__c}" label="Contact Name"/>
                               </div>
                            </div>
                            <div class="md-subhead" layout="row">
                                <div class="form-group"> 
                                    <label>City :&nbsp;</label>
                                    <apex:outputText value="{!account.Provider_City__r.Name}" label="Contact Name"/>
                                </div>
                            </div>
                        </div>
                            <div class="col-lg-6">
                                <div class="md-subhead" layout="row">
                                     <div class="form-group">
                                        <label>State :&nbsp;</label>
                                        <apex:outputField styleClass="form-control" value="{!account.State__c}" label="Contact Name"/>
                                    </div> 
                                </div>   
                                <div class="md-subhead" layout="row">    
                                    <div class="form-group">
                                        <label>Zip Code :&nbsp;</label>
                                        <apex:outputField styleClass="form-control" value="{!account.Provider_Zip_Code__c}" label="Contact Name"/>
                                    </div>
                                </div> 
                            </div>    
                        </div>
                    </section>
                    <section class="contact-information">
                        <h4 class="md-title">Mailing Address Information</h4>
                        <md-divider></md-divider><br/>
                        <div class="row">
                        <div class="col-lg-6">
                            <div class="md-subhead" layout="row">
                                <div class="form-group">
                                   <label>Street Address 1 </label>
                                   <apex:inputField styleClass="form-control" value="{!account.ShippingStreet}" label="Contact Name" style="min-height:65px;"/>
                               </div>
                            </div>
                            <div class="md-subhead" layout="row">    
                               <div class="form-group">
                                <label>Street Address 2 </label>
                                <apex:inputField styleClass="form-control" value="{!account.ShippingCity}" label="Contact Name"/>
                               </div>
                            </div>
                        </div>
                            <div class="col-lg-6">
                                <div class="md-subhead" layout="row" style="min-height: 98px;">
                                     <div class="form-group">
                                        <label>City </label>
                                        <apex:inputField styleClass="form-control" value="{!account.ShippingState}" label="Contact Name"/>
                                    </div> 
                                </div>   
                                <div class="md-subhead" layout="row">    
                                    <div class="form-group">
                                        <label>State </label>
                                        <apex:inputField styleClass="form-control" value="{!account.ShippingPostalCode}" label="Contact Name"/>
                                    </div>
                                </div> 
                            </div>    
                        </div>
            </section>
                <div class="btn-group pull-right">
                        <md-content class="navigation-buttons" layout="column" layout-lg="row" layout-align="space-between center" layout-gt-lg="row" layout-align-sm="center center" layout-align-md="center center">    
                            <apex:commandButton id="save" value="{!$Label.site.save}" styleClass="md-button md-raised md-eec pull-right" action="{!save}" rendered="{!isEdit}"/> 
                            <apex:commandButton id="cancel" value="{!$Label.site.cancel}" styleClass="md-button md-raised md-eec pull-right" action="{!cancel}"  rendered="{!isEdit}"/>            
                        </md-content>
                 </div>
            </apex:pageBlock>
            
          
        <apex:pageBlock id="viewUserDetail" rendered="{!!isEdit}" >
        <div class="row">
        <div class="col-lg-12">
            <section class="contact-information">
                <h4 class="md-title">Provider Information</h4>
                <md-divider></md-divider><br/>
                <div class="row">
                <div class="col-lg-6">
                    <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Name: </strong>{!account.name}</p>
                    </div>
                    
                    <apex:outputPanel rendered="{!IF(account.Recordtype.Name = 'Family Child Care', false,true)}">
                      <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Umbrella:</strong> <a href="/EEC_Umbrella" target="_top">{!account.Umbrella_Members__r.Name}</a></p>
                      </div>
                    </apex:outputPanel>
                    
                    <apex:outputPanel rendered="{!IF(account.Recordtype.Name != 'Family Child Care', false,true)}">
                      <div class="md-subhead" layout="row">
                        <p flex-sm="60" flex="70"><strong>FCC System:</strong> {!FCCSystemName}</p>  
                        <p flex-sm="40" flex="30"><a href="mailto:{!user.Email}?Subject=Change%20of%20FCC%20System%20request%20from%20{!HTMLENCODE(account.Name)}">Request a change</a></p>
                      </div>  
                    </apex:outputPanel>
                    
                    <apex:outputpanel rendered="{!IF(account.Recordtype.Name = 'Family Child Care', false,true)}">
                      <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Business Name:</strong>{!account.Business_Name__c}</p>
                      </div>
                    </apex:outputpanel>
                    
                    <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Type: </strong>{!account.Recordtype.Name}</p>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="md-subhead" layout="row">
                      <p flex-sm="80" flex="90"><strong>Number:</strong> {!account.Provider_Number__c}</p>
                    </div>
                    <div class="md-subhead" layout="row">
                      <p flex-sm="80" flex="90"><strong>License Status:</strong> {!account.License_Status__c}</p>
                    </div>
                    <div class="md-subhead" layout="row">
                      <!--<p flex-sm="80" flex="90"><strong>Tax ID:</strong> {!account.Tax_ID__c}</p>-->
                    </div> 
                </div>
                </div>    
            </section>
            </div>
            <br/>
            </div>
            <div class="row">
            <div class="col-lg-12">
                <section class="contact-information">
                <h4 class="md-title">Licensor</h4>
                <md-divider></md-divider><br/>
                <div class="row">
                <div class="col-lg-12">
                    <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>First / Last Name: </strong>{!user.name}</p>
                    </div>
                    <div class="md-subhead" layout="row">
                      <p flex-sm="80" flex="90"><strong>Phone: </strong>{!user.phone}</p>
                    </div>
                    <div class="md-subhead" layout="row">
                        <p flex-sm="80" flex="90"><strong>Email: </strong>{!user.Email}</p>
                    </div>
                </div>
                </div>    
            </section>
            </div>
            </div>

            <br/>    
        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
          <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#otherInfo" href="#otherInfo" aria-expanded="false" aria-controls="otherInfo" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Other Information
                </a>
              </h4>
            </div>
            <div id="otherInfo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div class="col-lg-6">
                       <div class="form-group">
                           <div class="input-group"> <span class="form-label">Public Approval: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Public_Approval__c}" />
                           </div>
                       </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Publish on Web: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Publish_on_Web__c}" />
                           </div>
                       </div>
                  </div>
                  <div class="col-lg-6">
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Head Start:  </span>
                           <apex:outputField styleClass="form-control" value="{!account.Head_Start__c}" />
                           </div>
                      </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Accept Referal:  </span>
                           <apex:outputField styleClass="form-control" value="{!account.Accept_Referral__c}" />
                           </div>
                       </div>
                  </div> 
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#providerContact" href="#providerContact" aria-expanded="false" aria-controls="providerContact" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Provider Contact Information
                </a>
              </h4>
            </div>
            <div id="providerContact" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div class="panel-body">
                  <div class="col-lg-6">
                       <div class="form-group">
                           <div class="input-group"> <span class="form-label">Phone:  </span>
                           <apex:outputField styleClass="form-control" value="{!account.Phone}" />
                           </div>
                       </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Alternate Phone: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Alternate_Phone__c}" />
                           </div>
                       </div>
                  </div>
                  <div class="col-lg-6">
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Fax: </span>
                           <apex:outputField styleClass="form-control" value="{!account.fax}" />
                           </div>
                      </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Email: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Provider_Email__c}" />
                           </div>
                       </div>
                  </div> 
              </div>
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#mainAddress" href="#mainAddress" aria-expanded="false" aria-controls="mainAddress" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Main Address Information
                </a>
              </h4>
            </div>
            <div id="mainAddress" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div class="col-lg-6">
                       <div class="form-group">
                           <div class="input-group"> <span class="form-label">Street Address 1: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Provider_Address_1__c}" />
                           </div>
                       </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Street Address 2: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Provider_Address_2__c}" />
                           </div>
                       </div>
                  </div>
                  <div class="col-lg-6">
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">City: </span>
                           <apex:outputText value="{!account.Provider_City__r.Name}" />
                           </div>
                      </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">State: </span>
                           <apex:outputField styleClass="form-control" value="{!account.State__c}" />
                           </div>
                       </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Zip Code: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Provider_Zip_Code__c}" />
                           </div>
                       </div>
                  </div> 
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#mailingAddress" href="#mailingAddress" aria-expanded="false" aria-controls="mailingAddress" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Mailing Address Information
                </a>
              </h4>
            </div>
            <div id="mailingAddress" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div class="col-lg-6">
                       <div class="form-group">
                           <div class="input-group"> <span class="form-label">Street Address: </span>
                           <apex:outputField styleClass="form-control" value="{!account.ShippingStreet}" />
                           </div>
                       </div>
                  </div>
                  <div class="col-lg-6">
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">City: </span>
                           <apex:outputField styleClass="form-control" value="{!account.ShippingCity}" />
                           </div>
                       </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">State: </span>
                           <apex:outputField styleClass="form-control" value="{!account.ShippingState}" />
                           </div>
                      </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Zip Code: </span>
                           <apex:outputField styleClass="form-control" value="{!account.ShippingPostalCode}" />
                           </div>
                       </div>
                  </div> 
              </div>
            </div>
          </div>
          <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#licenseInfo" href="#licenseInfo" aria-expanded="false" aria-controls="licenseInfo" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; License Information
                </a>
              </h4>
            </div>
            <div id="licenseInfo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <div class="panel-body">
                  <div class="col-lg-4">
                      <apex:outputPanel rendered="{!IF(account.Recordtype.Name = 'Umbrella' || account.Recordtype.Name = 'Unlicensed Provider', true,false)}">
                       <div class="form-group">
                           <div class="input-group"> <span class="form-label">Current License Number: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Current_License_Number__c}"/>
                           </div>
                       </div>
                      </apex:outputPanel>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">License Type: </span>
                           <apex:outputField styleClass="form-control" value="{!account.License_Type__c}" />
                           </div>
                       </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Expiration Date: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Renewal_date__c}" />
                           </div>
                      </div>
                  </div>
                  <div class="col-lg-4">
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">First Issue Date: </span>
                           <apex:outputField styleClass="form-control" value="{!account.First_Issue_Date__c}" />
                           </div>
                      </div>
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Last Issue Date: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Last_Issue_Date__c}" />
                           </div>
                       </div>
                  </div>
                  <div class="col-lg-4">
                      <div class="form-group">
                           <div class="input-group"> <span class="form-label">Capacity: </span>
                           <apex:outputField styleClass="form-control" value="{!account.Capacity__c}" />
                           </div>
                       </div>
                      <div class="form-group">
                          <div class="input-group"> <span class="form-label">License Conditions: </span>
                           <apex:outputpanel >
                               <apex:outputText value="{!conditionStr }" /> 
                          </apex:outputpanel>
                          </div>
                      </div>
                  </div>
              </div>
              </div>
            </div>
          </div>  
          <div class="panel panel-default">
            <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#programDetail" href="#programDetail" aria-expanded="false" aria-controls="programDetail" role="tab" id="headingThree">
              <h4 class="panel-title">
                <a>
                  <i class="fa fa-chevron-right"></i>&nbsp; Program Detailed Information
                </a>
              </h4>
            </div>
            <div id="programDetail" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body">
                  <apex:outputpanel rendered="{!IF(account.Recordtype.Name = 'Family Child Care', true,false)}">
                      <div class="col-lg-12">
                          <div class="col-lg-4">
                              <div class="form-group">
                                   <div class="input-group"> <span class="form-label">Foster Parent </span>
                                   <apex:outputField styleClass="form-control" value="{!account.Foster_Parent__c}" />
                                   </div>
                               </div>
                               <div class="form-group">
                                   <div class="input-group"> <span class="form-label">Bi-lingual Licensor </span>
                                   <apex:outputField styleClass="form-control" value="{!account.Bi_lingual_Licensor__c}" rendered="{!IF(account.Recordtype.Name = 'Family Child Care', true,false)}"/>
                                   </div>
                               </div>
                          </div>
                          <div class="col-lg-4">
